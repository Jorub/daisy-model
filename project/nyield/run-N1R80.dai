;;; run-N1R80.dai --- run N1994 scenarios with 80% root speed.

(input file "common-first.dai")

(scn "N1R80")

(defcrop "WW" "Winter Wheat; N1994"
  (Root (PenPar1 0.80 [mm/dg C/d])))
     
(defcrop "SB" "Spring Barley; N1994")
(defcrop "WR" "Winter Rape")

(input file "common-last.dai")

(run All)

;;; run-N1R80.dai ends here.

