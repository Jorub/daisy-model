(defgnuplot plot_N xy
  (where "N-yield.pdf")
  (source (frequency (sort sorg_N)
                     (title "JB4")
                     (file "JB4_crop-nyield.dlf"))
          (frequency (sort sorg_N)
                     (title "JB6")
                     (file "JB6_crop-nyield.dlf"))
          (frequency (sort sorg_N)
                     (title "JB7")
                     (file "JB7_crop-nyield.dlf"))))

