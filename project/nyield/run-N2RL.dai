;;; run-N2RL.dai --- run N2014 scenarios with smaller root zone.

(input file "common-first.dai")

(scn "N2RL")

(defcrop "WW" "Winter Wheat; N2014")
(defcrop "SB" "Spring Barley; N2014")
(defcrop "WR" "Winter Rape; PA")

(input file "common-last.dai")

(JB4_root 120 [cm])
(JB6_root 140 [cm])
(JB7_root 140 [cm])

(run All)

;;; run-N2RL.daiRL.dai ends here.

