;;; run-MXNRT10.dai --- run N1994 scenarios with max N uptake rate.

(input file "common-first.dai")

(scn "MXNRT10")

(defcrop "WW" "Winter Wheat; N1994"
  (Root (MxNH4Up     2.5E-8)
        (MxNO3Up     2.5E-8)))
     
(defcrop "SB" "Spring Barley; N1994"
  (Root (MxNH4Up     2.5E-8)
        (MxNO3Up     2.5E-8)))

(defcrop "WR" "Winter Rape; PA"
  (Root (MxNH4Up     2.5E-8)
        (MxNO3Up     2.5E-8)))

(input file "common-last.dai")

(run All)

;;; run-MXNRT10.dai ends here.

